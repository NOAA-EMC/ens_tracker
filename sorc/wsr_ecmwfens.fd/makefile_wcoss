SHELL=  /bin/bash
COMP=   ifort
FFLAGS= -r8 -i4 -list -traceback

wsr_ecmwfensh: ecmwfensh.f 
	@echo " "
	@echo "  Compiling program ECMWF GRIB1 to NCEP GRIB1...."
	$(COMP) $(FFLAGS) ecmwfensh.f $(LIBS) -o wsr_ecmwfensh
	@echo " "

CMD =   wsr_ecmwfensh

clean:
	-rm -f  *.o  *.mod

install:
	mv $(CMD) ../../exec/$(CMD)

